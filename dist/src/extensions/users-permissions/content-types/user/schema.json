{
    "kind": "collectionType",
    "collectionName": "up_users",
    "info": {
        "name": "user",
        "description": "",
        "singularName": "user",
        "pluralName": "users",
        "displayName": "User"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "username": {
            "type": "string",
            "minLength": 3,
            "unique": true,
            "configurable": false,
            "required": true
        },
        "email": {
            "type": "email",
            "minLength": 6,
            "configurable": false,
            "required": true
        },
        "provider": {
            "type": "string",
            "configurable": false
        },
        "password": {
            "type": "password",
            "minLength": 6,
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "resetPasswordToken": {
            "type": "string",
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "confirmationToken": {
            "type": "string",
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "confirmed": {
            "type": "boolean",
            "default": false,
            "configurable": false
        },
        "blocked": {
            "type": "boolean",
            "default": false,
            "configurable": false
        },
        "role": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.role",
            "inversedBy": "users",
            "configurable": false
        },
        "nickname": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "points": {
            "type": "integer",
            "default": 1000
        },
        "level": {
            "type": "integer",
            "default": 1
        },
        "lastAttendance": {
            "type": "datetime"
        },
        "attendanceStreak": {
            "type": "integer",
            "default": 0
        },
        "exp": {
            "type": "integer",
            "default": 0
        },
        "comments": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::comment.comment"
        },
        "votes": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::vote.vote"
        },
        "posts": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::post.post",
            "inversedBy": "users_permissions_user"
        },
        "point_histories": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::point-history.point-history",
            "mappedBy": "user"
        },
        "attendances": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::attendance.attendance",
            "mappedBy": "user"
        }
    }
}
